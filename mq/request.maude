fmod REQUEST is
    pr CONVERSION .
    sort Request .
    subsort String < Request .
    op getJob : -> Request .
    eq getJob = "getJob" .
    op sendJob : -> Request .
    eq sendJob = "sendJob" .
    op waiting : -> Request .
    eq waiting = "waiting" .
    op done : -> Request .
    eq done = "done" .
    op initialJob : -> Request .
    eq initialJob = "initialJob" .
endfm